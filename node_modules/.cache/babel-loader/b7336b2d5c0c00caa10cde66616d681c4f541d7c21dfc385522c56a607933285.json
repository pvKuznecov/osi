ols\">\r\n      <div class=\"url-bar\">\r\n        <select v-model=\"selectedProtocol\" style=\"margin-right: 5px;\">\r\n          <option value=\"https://\">https://</option>\r\n          <option value=\"http://\">http://</option>\r\n        </select>\r\n        <input\r\n          v-model=\"currentPath\"\r\n          @keyup.enter=\"loadUrl\"\r\n          placeholder=\"google.com\"\r\n        />\r\n        <button @click=\"loadUrl\">Перейти</button>\r\n      </div>\r\n    </div>\r\n    \r\n    <!-- Используем embed вместо iframe -->\r\n    <embed\r\n      v-if=\"useEmbed\"\r\n      :src=\"iframeSrc\"\r\n      style=\"width:100%; height:calc(100% - 60px)\"\r\n      type=\"text/html\"\r\n    />\r\n    \r\n    <!-- Или webview для Electron -->\r\n    <webview\r\n      v-else-if=\"isElectron\"\r\n      :src=\"iframeSrc\"\r\n      style=\"width:100%; height:calc(100% - 60px)\"\r\n    ></webview>\r\n    \r\n    <!-- Обычный iframe для остальных случаев -->\r\n    <iframe\r\n      v-else\r\n      :src=\"iframeSrc\"\r\n      sandbox=\"allow-scripts allow-same-origin allow-forms allow-popups\"\r\n      style=\"width:100%; height:calc(100% - 60px)\"\r\n      @load=\"onIframeLoad\"\r\n      @error=\"onIframeError\"\r\n    ></iframe>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: 'OSIBrowser',\r\n  props: {\r\n    // Можно передать стартовый URL через пропсы\r\n    initialUrl: {\r\n      type: String,\r\n      default: 'https://www.google.com'\r\n    }\r\n  },\r\n  \r\n  data() {\r\n    return {\r\n      currentUrl: this.initialUrl,\r\n      iframeSrc: this.initialUrl,\r\n      loading: false,\r\n      history: [this.initialUrl],\r\n      currentHistoryIndex: 0,\r\n      canGoBack: false,\r\n      canGoForward: false\r\n    };\r\n  },\r\n  \r\n  methods: {\r\n    loadUrl() {\r\n      let url = this.currentUrl.trim();\r\n      \r\n      // Добавляем протокол, если его нет\r\n      if (!url.startsWith('http://') && !url.startsWith('https://')) {\r\n        url = 'https://' + url;\r\n      }\r\n      \r\n      this.iframeSrc = url;\r\n      this.loading = true;\r\n      \r\n      // Добавляем в историю, если это новый URL\r\n      if (url !== this.history[this.currentHistoryIndex]) {\r\n        this.history = this.history.slice(0, this.currentHistoryIndex + 1);\r\n        this.history.push(url);\r\n        this.currentHistoryIndex = this.history.length - 1;\r\n        this.updateNavigationState();\r\n      }\r\n    },\r\n    \r\n    goBack() {\r\n      if (this.currentHistoryIndex > 0) {\r\n        this.currentHistoryIndex--;\r\n        this.iframeSrc = this.history[this.currentHistoryIndex];\r\n        this.currentUrl = this.iframeSrc;\r\n        this.loading = true;\r\n        this.updateNavigationState();\r\n      }\r\n    },\r\n    \r\n    goForward() {\r\n      if (this.currentHistoryIndex < this.history.length - 1) {\r\n        this.currentHistoryIndex++;\r\n        this.iframeSrc = this.history[this.currentHistoryIndex];\r\n        this.currentUrl = this.iframeSrc;\r\n        this.loading = true;\r\n        this.updateNavigationState();\r\n      }\r\n    },\r\n    \r\n    reload() {\r\n      this.loading = true;\r\n      this.$refs.iframeRef.contentWindow.location.reload();\r\n    },\r\n    \r\n    onIframeLoad() {\r\n      this.loading = false;\r\n      try {\r\n        // Обновляем URL из iframe (если разрешено политикой CORS)\r\n        const iframeUrl = this.$refs.iframeRef.contentWindow.location.href;\r\n        this.currentUrl = iframeUrl;\r\n      } catch (e) {\r\n        // CORS ошибка - игнорируем\r\n        console.log('Не могу получить URL из iframe из-за политики CORS');\r\n      }\r\n    },\r\n    \r\n    updateNavigationState() {\r\n      this.canGoBack = this.currentHistoryIndex > 0;\r\n      this.canGoForward = this.currentHistoryIndex < this.history.length - 1;\r\n    }\r\n  },\r\n  \r\n  mounted() {\r\n    this.loading = true;\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.browser-app {\r\n  width: 100%;\r\n  height: 100%;\r\n  display: flex;\r\n  flex-direction: column;\r\n}\r\n\r\n.browser-controls {\r\n  padding: 10px;\r\n  background: #f5f5f5;\r\n  border-bottom: 1px solid #ddd;\r\n}\r\n\r\n.url-bar {\r\n  display: flex;\r\n  gap: 5px;\r\n  align-items: center;\r\n}\r\n\r\n.url-bar input {\r\n  flex: 1;\r\n  padding: 8px 12px;\r\n  border: 1px solid #ccc;\r\n  border-radius: 4px;\r\n  font-size: 14px;\r\n}\r\n\r\n.url-bar button {\r\n  padding: 8px 12px;\r\n  border: 1px solid #ccc;\r\n  background: white;\r\n  border-radius: 4px;\r\n  cursor: pointer;\r\n  transition: background-color 0.2s;\r\n}\r\n\r\n.url-bar button:hover:not(:disabled) {\r\n  background: #e9e9e9;\r\n}\r\n\r\n.url-bar button:disabled {\r\n  opacity: 0.5;\r\n  cursor: not-allowed;\r\n}\r\n\r\n.iframe-container {\r\n  flex: 1;\r\n  position: relative;\r\n  overflow: hidden;\r\n}\r\n\r\n.iframe-container iframe {\r\n  width: 100%;\r\n  height: 100%;\r\n  border: none;\r\n  display: block;\r\n}\r\n\r\n.loading-overlay {\r\n  position: absolute;\r\n  top: 0;\r\n  left: 0;\r\n  right: 0;\r\n  bottom: 0;\r\n  background: rgba(255, 255, 255, 0.8);\r\n  display: flex;\r\n  align-items: center;\r\n  justify-content: center;\r\n  font-size: 16px;\r\n  color: #666;\r\n}\r\n</style>"],"mappings":";;EACOA,KAAK,EAAC;AAAa;;EACjBA,KAAK,EAAC;AAAkB;;EACtBA,KAAK,EAAC;AAAS;;;;;uBAFxBC,mBAAA,CAwCM,OAxCNC,UAwCM,GAvCJC,mBAAA,CAaM,OAbNC,UAaM,GAZJD,mBAAA,CAWM,OAXNE,UAWM,G,gBAVJF,mBAAA,CAGS;+DAHQG,IAAA,CAAAC,gBAAgB,GAAAC,MAAA;IAAEC,KAA0B,EAA1B;MAAA;IAAA;qCACjCN,mBAAA,CAA0C;IAAlCO,KAAK,EAAC;EAAU,GAAC,UAAQ,oBACjCP,mBAAA,CAAwC;IAAhCO,KAAK,EAAC;EAAS,GAAC,SAAO,mB,6CAFhBJ,IAAA,CAAAC,gBAAgB,E,mBAIjCJ,mBAAA,CAIE;+DAHSG,IAAA,CAAAK,WAAW,GAAAH,MAAA;IACnBI,OAAK,EAAAC,MAAA,QAAAA,MAAA,MAAAC,SAAA,KAAAC,IAAA,KAAQC,QAAA,CAAAC,OAAA,IAAAD,QAAA,CAAAC,OAAA,IAAAF,IAAA,CAAO;IACrBG,WAAW,EAAC;iEAFHZ,IAAA,CAAAK,WAAW,E,GAItBR,mBAAA,CAAyC;IAAhCgB,OAAK,EAAAN,MAAA,QAAAA,MAAA,UAAAE,IAAA,KAAEC,QAAA,CAAAC,OAAA,IAAAD,QAAA,CAAAC,OAA