{"ast":null,"code":"// import OSIHelper from '@/apps/system/OSIHelper/OSIHelper.vue';\n\nexport default {\n  name: \"SimpleWindow\",\n  // components: {\n  //     OSIHelper\n  // },\n\n  props: {\n    windowId: String,\n    title: {\n      type: String,\n      default: 'Окно'\n    },\n    appName: {\n      type: String,\n      default: 'app'\n    },\n    contentApp: {\n      type: String,\n      default: '--'\n    },\n    isMinimized: {\n      type: Boolean,\n      default: false\n    },\n    isMaximized: {\n      type: Boolean,\n      default: false\n    },\n    isActive: {\n      type: Boolean,\n      default: false\n    },\n    zIndex: {\n      type: Number,\n      default: 100\n    }\n  },\n  emits: ['close', 'minimize', 'focus', 'toggleMaximize'],\n  data() {\n    return {\n      posX: 100,\n      posY: 100,\n      windowWidth: 400,\n      windowHeight: 300,\n      isDragging: false,\n      isResizing: false,\n      dragOffset: {\n        x: 0,\n        y: 0\n      },\n      resizeDirection: '',\n      resizeStart: {\n        x: 0,\n        y: 0,\n        width: 0,\n        height: 0,\n        mouseX: 0,\n        mouseY: 0\n      }\n    };\n  },\n  computed: {\n    dynamicComponent() {\n      // Создаем маппинг имен компонентов\n      const componentMap = {\n        'OSIHelper': () => import('@/apps/system/OSIHelper/OSIHelper.vue'),\n        'Notepad': () => import('@/apps/NotepadApp.vue'),\n        'Explorer': () => import('@/apps/ExplorerApp.vue'),\n        'Calculator': () => import('@/apps/CalculatorApp.vue'),\n        'Settings': () => import('@/apps/SettingsApp.vue')\n      };\n      if (this.contentApp && componentMap[this.contentApp]) {\n        return defineAsyncComponent(componentMap[this.contentApp]);\n      }\n      return null;\n    },\n    windowStyles() {\n      const baseStyles = {\n        zIndex: this.zIndex\n      };\n      if (this.isMaximized) {\n        return {\n          ...baseStyles,\n          left: '0',\n          top: '0',\n        