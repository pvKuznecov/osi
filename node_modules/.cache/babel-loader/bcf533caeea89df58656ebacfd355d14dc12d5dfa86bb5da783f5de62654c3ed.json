{"ast":null,"code":"import \"core-js/modules/es.iterator.constructor.js\";\nimport \"core-js/modules/es.iterator.find.js\";\nimport { useOsStore } from '@/stores/os.store';\nimport { appConfig } from '@/config/applications';\nexport default {\n  name: \"TaskBar\",\n  data() {\n    return {\n      currentTime: '',\n      timer: null\n    };\n  },\n  computed: {\n    osStore() {\n      return useOsStore();\n    }\n  },\n  methods: {\n    updateTime() {\n      const now = new Date();\n      this.currentTime = now.toLocaleTimeString('ru-RU', {\n        hour: '2-digit',\n        minute: '2-digit'\n      });\n    },\n    // getAppIcon(appName) {\n    //     const icons = {\n    //         notepad: 'üìù',\n    //         explorer: 'üìÅ',\n    //         calculator: 'üßÆ',\n    //         settings: '‚öôÔ∏è',\n    //         osihelper: '‚ÑπÔ∏è'\n    //     };\n\n    //     return icons[appName] || 'üìÑ';\n    // },\n    getAppIcon(appName) {\n      // –ò—Å–ø–æ–ª—å–∑—É–µ–º –∫–æ–Ω—Ñ–∏–≥ –≤–º–µ—Å—Ç–æ —Ö–∞—Ä–¥–∫–æ–¥–∞\n      const app = appConfig.getAppByName(appName);\n      return app?.icon || 'üìÑ';\n    },\n    toggleWindow(windowId) {\n      const window = this.osStore.windows.find(w => w.id === windowId);\n      if (window) {\n        if (window.isMinimized) {\n          this.osStore.restoreWindow(windowId);\n        } else {\n          this.osStore.activateWindow(windowId);\n        }\n      }\n    },\n    toggleStartMenu() {\n      console.log('–ú–µ–Ω—é –ü—É—Å–∫');\n    }\n  },\n  mounted() {\n    this.updateTime();\n    this.timer = setInterval(this.updateTime, 1000);\n  },\n  beforeUnmount() {\n    if (this.timer) {\n      clearInterval(this.timer);\n    }\n  }\n};","map":{"version":3,"names":["useOsStore","appConfig","name","data","currentTime","timer","computed","osStore","methods","updateTime","now","Date","toLocaleTimeString","hour","minute","getAppIcon","appName","app","getAppByName","icon","toggleWindow","windowId","window","windows","find","w","id","isMinimized","restoreWindow","activateWindow","toggleStartMenu","console","log","mounted","setInterval","beforeUnmount","clearInterval"],"sources":["/home/kup/my_projects/vue/osi/src/components/os/TaskBar/TaskBar.vue"],"sourcesContent":["<template>\n    <div class=\"task-bar\">\n        <!-- –ö–Ω–æ–ø–∫–∞ Menu -->\n        <button class=\"start-button\" @click=\"toggleStartMenu\">\n            <span class=\"start-icon\">üåÄ</span>\n            <span class=\"start-text\">–ú–µ–Ω—é</span>\n        </button>    \n        <!-- –û—Ç–∫—Ä—ã—Ç—ã–µ –ø—Ä–∏–ª–æ–∂–µ–Ω–∏—è -->\n        <div class=\"taskbar-apps\">\n            <button v-for=\"window in osStore.windows\" :key=\"window.id\" class=\"taskbar-app\" :class=\"{ 'active': osStore.activeWindowId === window.id }\" @click=\"toggleWindow(window.id)\">\n                <span class=\"app-icon\">{{ getAppIcon(window.appName) }}</span>\n                <span class=\"app-title\">{{ window.title }}</span>\n            </button>\n        </div>    \n        <!-- –°–∏—Å—Ç–µ–º–Ω—ã–π —Ç—Ä–µ–π -->\n        <div class=\"system-tray\">\n            <div class=\"tray-time\">{{ currentTime }}</div>\n        </div>\n    </div>\n</template>\n<script>\n    import { useOsStore } from '@/stores/os.store';\n    import { appConfig } from '@/config/applications';\n\n    export default {\n        name: \"TaskBar\",\n  \n        data() {\n            return {\n                currentTime: '',\n                timer: null\n            };\n        },\n  \n        computed: {\n            osStore() {\n                return useOsStore();\n            }\n        },\n  \n        methods: {\n            updateTime() {\n                const now = new Date();\n                this.currentTime = now.toLocaleTimeString('ru-RU', {hour: '2-digit', minute: '2-digit'});\n            },\n    \n            // getAppIcon(appName) {\n            //     const icons = {\n            //         notepad: 'üìù',\n            //         explorer: 'üìÅ',\n            //         calculator: 'üßÆ',\n            //         settings: '‚öôÔ∏è',\n            //         osihelper: '‚ÑπÔ∏è'\n            //     };\n\n            //     return icons[appName] || 'üìÑ';\n            // },\n            getAppIcon(appName) {\n                // –ò—Å–ø–æ–ª—å–∑—É–µ–º –∫–æ–Ω—Ñ–∏–≥ –≤–º–µ—Å—Ç–æ —Ö–∞—Ä–¥–∫–æ–¥–∞\n                const app = appConfig.getAppByName(appName);\n                return app?.icon || 'üìÑ';\n            },\n    \n            toggleWindow(windowId) {\n                const window = this.osStore.windows.find(w => w.id === windowId);\n                \n                if (window) {\n                    if (window.isMinimized) {\n                        this.osStore.restoreWindow(windowId);\n                    } else {\n                        this.osStore.activateWindow(windowId);\n                    }\n                }\n            },\n    \n            toggleStartMenu() {\n                console.log('–ú–µ–Ω—é –ü—É—Å–∫');\n            }\n        },\n  \n        mounted() {\n            this.updateTime();\n            this.timer = setInterval(this.updateTime, 1000);\n        },\n  \n        beforeUnmount() {\n            if (this.timer) {\n                clearInterval(this.timer);\n            }\n        }\n    }\n</script>\n<style>\n.task-bar {\n    display: flex;\n    height: 40px;\n    background: linear-gradient(rgba(0, 0, 0, 0.85), rgba(0, 0, 0, 0.8), rgba(0, 0, 0, 0.85));\n    color: white;\n    align-items: center;\n    padding: 0 10px;\n    gap: 10px;\n    backdrop-filter: blur(10px);\n    border-top: 1px solid rgba(255, 255, 255, 0.1); }\n\n.start-button {\n    display: flex;\n    height: 30px;\n    padding: 0 12px;\n    background: linear-gradient(to bottom, rgb(103, 55, 235), rgb(80, 33, 209));\n    border: none;\n    border-radius: 4px;\n    color: white;\n    align-items: center;\n    gap: 5px;\n    cursor: pointer;\n    font-size: 14px;\n    transition: all 0.2s; }\n\n.start-button:hover {\n    background: linear-gradient(to bottom, rgb(112, 69, 231), rgb(92, 49, 211));\n    transform: translateY(-1px); }\n\n.start-icon { font-size: 16px; }\n\n.taskbar-apps {\n    display: flex;\n    gap: 5px;\n    flex: 1;\n    overflow-x: auto;\n    padding: 0 5px; }\n\n.taskbar-app {\n    display: flex;\n    min-width: 120px;\n    height: 30px;  \n    padding: 0 12px;\n    background: rgba(255, 255, 255, 0.192);\n    border: none;\n    border-radius: 4px;\n    color: white;  \n    align-items: center;\n    gap: 6px;\n    cursor: pointer;\n    font-size: 12px;\n    white-space: nowrap;\n    transition: all 0.2s; }\n\n.taskbar-app:hover { background: rgba(255, 255, 255, 0.2); }\n\n.taskbar-app.active {\n    background: white;\n    color: black; }\n\n.app-icon { font-size: 14px; }\n\n.system-tray {\n    display: flex;\n    align-items: center;\n    gap: 10px;\n    padding-left: 10px;\n    border-left: 1px solid rgba(255, 255, 255, 0.1); }\n\n.tray-time {\n    font-size: 12px;\n    font-weight: 500;\n    padding: 4px 8px;\n    background: rgba(0, 0, 0, 0.3);\n    border-radius: 4px; }\n</style>"],"mappings":";;AAqBI,SAASA,UAAS,QAAS,mBAAmB;AAC9C,SAASC,SAAQ,QAAS,uBAAuB;AAEjD,eAAe;EACXC,IAAI,EAAE,SAAS;EAEfC,IAAIA,CAAA,EAAG;IACH,OAAO;MACHC,WAAW,EAAE,EAAE;MACfC,KAAK,EAAE;IACX,CAAC;EACL,CAAC;EAEDC,QAAQ,EAAE;IACNC,OAAOA,CAAA,EAAG;MACN,OAAOP,UAAU,CAAC,CAAC;IACvB;EACJ,CAAC;EAEDQ,OAAO,EAAE;IACLC,UAAUA,CAAA,EAAG;MACT,MAAMC,GAAE,GAAI,IAAIC,IAAI,CAAC,CAAC;MACtB,IAAI,CAACP,WAAU,GAAIM,GAAG,CAACE,kBAAkB,CAAC,OAAO,EAAE;QAACC,IAAI,EAAE,SAAS;QAAEC,MAAM,EAAE;MAAS,CAAC,CAAC;IAC5F,CAAC;IAED;IACA;IACA;IACA;IACA;IACA;IACA;IACA;;IAEA;IACA;IACAC,UAAUA,CAACC,OAAO,EAAE;MAChB;MACA,MAAMC,GAAE,GAAIhB,SAAS,CAACiB,YAAY,CAACF,OAAO,CAAC;MAC3C,OAAOC,GAAG,EAAEE,IAAG,IAAK,IAAI;IAC5B,CAAC;IAEDC,YAAYA,CAACC,QAAQ,EAAE;MACnB,MAAMC,MAAK,GAAI,IAAI,CAACf,OAAO,CAACgB,OAAO,CAACC,IAAI,CAACC,CAAA,IAAKA,CAAC,CAACC,EAAC,KAAML,QAAQ,CAAC;MAEhE,IAAIC,MAAM,EAAE;QACR,IAAIA,MAAM,CAACK,WAAW,EAAE;UACpB,IAAI,CAACpB,OAAO,CAACqB,aAAa,CAACP,QAAQ,CAAC;QA