{"ast":null,"code":"import DesktopArea from './components/os/DesktopArea/DesktopArea.vue';\nimport NotificationsArea from './components/os/NotificationsArea/NotificationsArea.vue';\nimport TaskBar from './components/os/TaskBar/TaskBar.vue';\nimport SimpleWindow from './components/os/SimpleWindow/SimpleWindow.vue';\nimport { useOsStore } from './stores/os.store';\nimport { useAppsStore } from './stores/apps.store';\nimport { useNotifStore } from './stores/notif.store';\nexport default {\n  name: 'App',\n  components: {\n    DesktopArea,\n    NotificationsArea,\n    TaskBar,\n    SimpleWindow\n  },\n  data() {\n    return {\n      osStore: null,\n      appsStore: null,\n      notifStore: null\n      // useAppsStore: null\n    };\n  },\n  created() {\n    this.osStore = useOsStore();\n    this.appsStore = useAppsStore();\n    this.notifStore = useNotifStore();\n    // this.useAppsStore = useAppsStore();\n\n    console.log('this.notifStore', this.notifStore);\n  },\n  methods: {\n    closeWindow(windowId) {\n      console.log(\"closeWindow IT!\");\n      if (this.osStore) {\n        this.osStore.closeWindow(windowId);\n      }\n      if (this.appsStore) {\n        this.appsStore.deleteWindowState(windowId);\n      }\n    },\n    minimizeWindow(windowId) {\n      if (this.osStore) {\n        this.osStore.minimizeWindow(windowId);\n      }\n    },\n    toggleMaximizeWindow(windowId) {\n      if (this.osStore) {\n        this.osStore.toggleMaximizeWindow(windowId);\n      }\n    },\n    activateWindow(windowId) {\n      if (this.osStore) {\n        this.osStore.activateWindow(windowId);\n      }\n    }\n  }\n\n  // mounted() {\n  // }\n};","map":{"version":3,"names":["DesktopArea","NotificationsArea","TaskBar","SimpleWindow","useOsStore","useAppsStore","useNotifStore","name","components","data","osStore","appsStore","notifStore","created","console","log","methods","closeWindow","windowId","deleteWindowState","minimizeWindow","toggleMaximizeWindow","activateWindow"],"sources":["/home/kup/my_projects/vue/osi/src/App.vue"],"sourcesContent":["<template>\n    <div class=\"webos\">\n        <DesktopArea>\n            <SimpleWindow v-for=\"window in osStore.windows\"\n                :key=\"window.id\"\n                :windowId=\"window.id\"\n                :title=\"window.title\"\n                :appName=\"window.appName\"\n        \n                :contentApp=\"window.contentApp\"\n                :icon=\"window.icon\"\n\n                :defWidth=\"window.defWidth\"\n                :defHeight=\"window.defHeight\"\n                :isMinimized=\"window.isMinimized\"\n                :isActive=\"osStore.activeWindowId === window.id\"\n                :isMaximized=\"window.isMaximized\"\n                :zIndex=\"window.zIndex\"\n                :resizable=\"window.resizable\"\n                :canMinimize=\"window.canMinimize\"\n                \n                @close=\"closeWindow\"\n                @minimize=\"minimizeWindow\"\n                @toggleMaximize=\"toggleMaximizeWindow\"\n                @focus=\"activateWindow\"\n            />\n        </DesktopArea>\n        <NotificationsArea :notifs=\"notifStore.notifs\"/>\n        <TaskBar />\n    </div>\n</template>\n<style scoped>\n    .webos { display: flex; flex-direction: column; height: 100vh; overflow: hidden; }\n</style>\n<script>\n    import DesktopArea from './components/os/DesktopArea/DesktopArea.vue';\n    import NotificationsArea from './components/os/NotificationsArea/NotificationsArea.vue';\n    import TaskBar from './components/os/TaskBar/TaskBar.vue';\n    import SimpleWindow from './components/os/SimpleWindow/SimpleWindow.vue';\n    import { useOsStore } from './stores/os.store';\n    import { useAppsStore } from './stores/apps.store';\n    import { useNotifStore } from './stores/notif.store';\n\n    export default {\n        name: 'App',\n  \n        components: {\n            DesktopArea,\n            NotificationsArea,\n            TaskBar,\n            SimpleWindow\n        },\n  \n        data() {\n            return {\n                osStore: null,\n                appsStore: null,\n                notifStore: null,\n                // useAppsStore: null\n            }\n        },\n\n        created() {\n            this.osStore = useOsStore();\n            this.appsStore = useAppsStore();\n            this.notifStore = useNotifStore();\n            // this.useAppsStore = useAppsStore();\n\n            console.log('this.notifStore', this.notifStore);\n        },\n  \n        methods: {\n            closeWindow(windowId) {\n                console.log(\"closeWindow IT!\");\n                if (this.osStore) {\n                    this.osStore.closeWindow(windowId);\n                }\n\n                if (this.appsStore) {\n                    this.appsStore.deleteWindowState(windowId);\n                }\n            },\n    \n            minimizeWindow(windowId) {\n                if (this.osStore) {\n                    this.osStore.minimizeWindow(windowId);\n                }\n            },\n    \n            toggleMaximizeWindow(windowId) {\n                if (this.osStore) {\n                    this.osStore.toggleMaximizeWindow(windowId);\n                }\n            },\n    \n            activateWindow(windowId) {\n                if (this.osStore) {\n                    this.osStore.activateWindow(windowId);\n                }\n            }\n        },\n  \n        // mounted() {\n        // }\n    }\n</script>"],"mappings":"AAmCI,OAAOA,WAAU,MAAO,6CAA6C;AACrE,OAAOC,iBAAgB,MAAO,yDAAyD;AACvF,OAAOC,OAAM,MAAO,qCAAqC;AACzD,OAAOC,YAAW,MAAO,+CAA+C;AACxE,SAASC,UAAS,QAAS,mBAAmB;AAC9C,SAASC,YAAW,QAAS,qBAAqB;AAClD,SAASC,aAAY,QAAS,sBAAsB;AAEpD,eAAe;EACXC,IAAI,EAAE,KAAK;EAEXC,UAAU,EAAE;IACRR,WAAW;IACXC,iBAAiB;IACjBC,OAAO;IACPC;EACJ,CAAC;EAEDM,IAAIA,CAAA,EAAG;IACH,OAAO;MACHC,OAAO,EAAE,IAAI;MACbC,SAAS,EAAE,IAAI;MACfC,UAAU,EAAE;MACZ;IACJ;EACJ,CAAC;EAEDC,OAAOA,CAAA,EAAG;IACN,IAAI,CAACH,OAAM,GAAIN,UAAU,CAAC,CAAC;IAC3B,