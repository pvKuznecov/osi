{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nexport default {\n  name: 'OSIBrowser',\n  props: {\n    // Можно передать стартовый URL через пропсы\n    initialUrl: {\n      type: String,\n      default: 'https://www.google.com'\n    }\n  },\n  data() {\n    return {\n      currentUrl: this.initialUrl,\n      iframeSrc: this.initialUrl,\n      loading: false,\n      history: [this.initialUrl],\n      currentHistoryIndex: 0,\n      canGoBack: false,\n      canGoForward: false\n    };\n  },\n  methods: {\n    loadUrl() {\n      let url = this.currentUrl.trim();\n\n      // Добавляем протокол, если его нет\n      if (!url.startsWith('http://') && !url.startsWith('https://')) {\n        url = 'https://' + url;\n      }\n      this.iframeSrc = url;\n      this.loading = true;\n\n      // Добавляем в историю, если это новый URL\n      if (url !== this.history[this.currentHistoryIndex]) {\n        this.history = this.history.slice(0, this.currentHistoryIndex + 1);\n        this.history.push(url);\n        this.currentHistoryIndex = this.history.length - 1;\n        this.updateNavigationState();\n      }\n    },\n    goBack() {\n      if (this.currentHistoryIndex > 0) {\n        this.currentHistoryIndex--;\n        this.iframeSrc = this.history[this.currentHistoryIndex];\n        this.currentUrl = this.iframeSrc;\n        this.loading = true;\n        this.updateNavigationState();\n      }\n    },\n    goForward() {\n      if (this.currentHistoryIndex < this.history.length - 1) {\n        this.currentHistoryIndex++;\n        this.iframeSrc = this.history[this.currentHistoryIndex];\n        this.currentUrl = this.iframeSrc;\n        this.loading = true;\n        this.updateNavigationState();\n      }\n    },\n    reload() {\n      this.loading = true;\n      this.$refs.iframeRef.contentWindow.location.reload();\n    },\n    onIframeLoad() {\n      this.loading = false;\n      try {\n        // Обновляем URL из iframe (если разрешено политикой CORS)\n        const iframeUrl = this.$refs.iframeRef.contentWindow.location.href;\n        this.currentUrl = iframeUrl;\n      } catch (e) {\n        // CORS ошибка - игнорируем\n        console.log('Не могу получить URL из iframe из-за политики CORS');\n      }\n    },\n    updateNavigationState() {\n      this.canGoBack = this.currentHistoryIndex > 0;\n      this.canGoForward = this.currentHistoryIndex < this.history.length - 1;\n    }\n  },\n  mounted() {\n    this.loading = true;\n  }\n};","map":{"version":3,"names":["name","props","initialUrl","type","String","default","data","currentUrl","iframeSrc","loading","history","currentHistoryIndex","canGoBack","canGoForward","methods","loadUrl","url","trim","startsWith","slice","push","length","updateNavigationState","goBack","goForward","reload","$refs","iframeRef","contentWindow","location","onIframeLoad","iframeUrl","href","e","console","log","mounted"],"sources":["C:\\projects\\My projects\\Vue\\osi\\src\\apps\\system\\OSIBrowser\\OSIBrowser.vue"],"sourcesContent":["<template>\r\n  <div class=\"browser-app\">\r\n    <div class=\"browser-controls\">\r\n      <div class=\"url-bar\">\r\n        <input\r\n          v-model=\"currentUrl\"\r\n          @keyup.enter=\"loadUrl\"\r\n          placeholder=\"Введите URL (например: https://google.com)\"\r\n        />\r\n        <button @click=\"loadUrl\">Перейти</button>\r\n        <button @click=\"goBack\" :disabled=\"!canGoBack\">←</button>\r\n        <button @click=\"goForward\" :disabled=\"!canGoForward\">→</button>\r\n        <button @click=\"reload\">⟳</button>\r\n      </div>\r\n    </div>\r\n    \r\n    <div class=\"iframe-container\">\r\n      <embed\r\n        ref=\"iframeRef\"\r\n        :src=\"iframeSrc\"\r\n        @load=\"onIframeLoad\"\r\n        sandbox=\"allow-same-origin allow-scripts allow-popups allow-forms\"\r\n        allow=\"geolocation; microphone; camera; payment; autoplay\"\r\n      />\r\n      <!-- <iframe\r\n        ref=\"iframeRef\"\r\n        :src=\"iframeSrc\"\r\n        @load=\"onIframeLoad\"\r\n        sandbox=\"allow-same-origin allow-scripts allow-popups allow-forms\"\r\n        allow=\"geolocation; microphone; camera; payment; autoplay\"\r\n      ></iframe> -->\r\n    </div>\r\n    \r\n    <div v-if=\"loading\" class=\"loading-overlay\">\r\n      Загрузка...\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: 'OSIBrowser',\r\n  props: {\r\n    // Можно передать стартовый URL через пропсы\r\n    initialUrl: {\r\n      type: String,\r\n      default: 'https://www.google.com'\r\n    }\r\n  },\r\n  \r\n  data() {\r\n    return {\r\n      currentUrl: this.initialUrl,\r\n      iframeSrc: this.initialUrl,\r\n      loading: false,\r\n      history: [this.initialUrl],\r\n      currentHistoryIndex: 0,\r\n      canGoBack: false,\r\n      canGoForward: false\r\n    };\r\n  },\r\n  \r\n  methods: {\r\n    loadUrl() {\r\n      let url = this.currentUrl.trim();\r\n      \r\n      // Добавляем протокол, если его нет\r\n      if (!url.startsWith('http://') && !url.startsWith('https://')) {\r\n        url = 'https://' + url;\r\n      }\r\n      \r\n      this.iframeSrc = url;\r\n      this.loading = true;\r\n      \r\n      // Добавляем в историю, если это новый URL\r\n      if (url !== this.history[this.currentHistoryIndex]) {\r\n        this.history = this.history.slice(0, this.currentHistoryIndex + 1);\r\n        this.history.push(url);\r\n        this.currentHistoryIndex = this.history.length - 1;\r\n        this.updateNavigationState();\r\n      }\r\n    },\r\n    \r\n    goBack() {\r\n      if (this.currentHistoryIndex > 0) {\r\n        this.currentHistoryIndex--;\r\n        this.iframeSrc = this.history[this.currentHistoryIndex];\r\n        this.currentUrl = this.iframeSrc;\r\n        this.loading = true;\r\n        this.updateNavigationState();\r\n      }\r\n    },\r\n    \r\n    goForward() {\r\n      if (this.currentHistoryIndex < this.history.length - 1) {\r\n        this.currentHistoryIndex++;\r\n        this.iframeSrc = this.history[this.currentHistoryIndex];\r\n        this.currentUrl = this.iframeSrc;\r\n        this.loading = true;\r\n        this.updateNavigationState();\r\n      }\r\n    },\r\n    \r\n    reload() {\r\n      this.loading = true;\r\n      this.$refs.iframeRef.contentWindow.location.reload();\r\n    },\r\n    \r\n    onIframeLoad() {\r\n      this.loading = false;\r\n      try {\r\n        // Обновляем URL из iframe (если разрешено политикой CORS)\r\n        const iframeUrl = this.$refs.iframeRef.contentWindow.location.href;\r\n        this.currentUrl = iframeUrl;\r\n      } catch (e) {\r\n        // CORS ошибка - игнорируем\r\n        console.log('Не могу получить URL из iframe из-за политики CORS');\r\n      }\r\n    },\r\n    \r\n    updateNavigationState() {\r\n      this.canGoBack = this.currentHistoryIndex > 0;\r\n      this.canGoForward = this.currentHistoryIndex < this.history.length - 1;\r\n    }\r\n  },\r\n  \r\n  mounted() {\r\n    this.loading = true;\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.browser-app {\r\n  width: 100%;\r\n  height: 100%;\r\n  display: flex;\r\n  flex-direction: column;\r\n}\r\n\r\n.browser-controls {\r\n  padding: 10px;\r\n  background: #f5f5f5;\r\n  border-bottom: 1px solid #ddd;\r\n}\r\n\r\n.url-bar {\r\n  display: flex;\r\n  gap: 5px;\r\n  align-items: center;\r\n}\r\n\r\n.url-bar input {\r\n  flex: 1;\r\n  padding: 8px 12px;\r\n  border: 1px solid #ccc;\r\n  border-radius: 4px;\r\n  font-size: 14px;\r\n}\r\n\r\n.url-bar button {\r\n  padding: 8px 12px;\r\n  border: 1px solid #ccc;\r\n  background: white;\r\n  border-radius: 4px;\r\n  cursor: pointer;\r\n  transition: background-color 0.2s;\r\n}\r\n\r\n.url-bar button:hover:not(:disabled) {\r\n  background: #e9e9e9;\r\n}\r\n\r\n.url-bar button:disabled {\r\n  opacity: 0.5;\r\n  cursor: not-allowed;\r\n}\r\n\r\n.iframe-container {\r\n  flex: 1;\r\n  position: relative;\r\n  overflow: hidden;\r\n}\r\n\r\n.iframe-container iframe {\r\n  width: 100%;\r\n  height: 100%;\r\n  border: none;\r\n  display: block;\r\n}\r\n\r\n.loading-overlay {\r\n  position: absolute;\r\n  top: 0;\r\n  left: 0;\r\n  right: 0;\r\n  bottom: 0;\r\n  background: rgba(255, 255, 255, 0.8);\r\n  display: flex;\r\n  align-items: center;\r\n  justify-content: center;\r\n  font-size: 16px;\r\n  color: #666;\r\n}\r\n</style>"],"mappings":";AAwCA,eAAe;EACbA,IAAI,EAAE,YAAY;EAClBC,KAAK,EAAE;IACL;IACAC,UAAU,EAAE;MACVC,IAAI,EAAEC,MAAM;MACZC,OAAO,EAAE;IACX;EACF,CAAC;EAEDC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,UAAU,EAAE,IAAI,CAACL,UAAU;MAC3BM,SAAS,EAAE,IAAI,CAACN,UAAU;MAC1BO,OAAO,EAAE,KAAK;MACdC,OAAO,EAAE,CAAC,IAAI,CAACR,UAAU,CAAC;MAC1BS,mBAAmB,EAAE,CAAC;MACtBC,SAAS,EAAE,KAAK;MAChBC,YAAY,EAAE;IAChB,CAAC;EACH,CAAC;EAEDC,OAAO,EAAE;IACPC,OAAOA,CAAA,EAAG;MACR,IAAIC,GAAE,GAAI,IAAI,CAACT,UAAU,CAACU,IAAI,CAAC,CAAC;;MAEhC;MACA,IAAI,CAACD,GAAG,CAACE,UAAU,CAAC,SAAS,KAAK,CAACF,GAAG,CAACE,UAAU,CAAC,UAAU,CAAC,EAAE;QAC7DF,GAAE,GAAI,UAAS,GAAIA,GAAG;MACxB;MAEA,IAAI,CAACR,SAAQ,GAAIQ,GAAG;MACpB,IAAI,CAACP,OAAM,GAAI,IAAI;;MAEnB;MACA,IAAIO,GAAE,KAAM,IAAI,CAACN,OAAO,CAAC,IAAI,CAACC,mBAAmB,CAAC,EAAE;QAClD,IAAI,CAACD,OAAM,GAAI,IAAI,CAACA,OAAO,CAACS,KAAK,CAAC,CAAC,EAAE,IAAI,CAACR,mBAAkB,GAAI,CAAC,CAAC;QAClE,IAAI,CAACD,OAAO,CAACU,IAAI,CAACJ,GAAG,CAAC;QACtB,IAAI,CAACL,mBAAkB,GAAI,IAAI,CAACD,OAAO,CAACW,MAAK,GAAI,CAAC;QAClD,IAAI,CAACC,qBAAqB,CAAC,CAAC;MAC9B;IACF,CAAC;IAEDC,MAAMA,CAAA,EAAG;MACP,IAAI,IAAI,CAACZ,mBAAkB,GAAI,CAAC,EAAE;QAChC,IAAI,CAACA,mBAAmB,EAAE;QAC1B,IAAI,CAACH,SAAQ,GAAI,IAAI,CAACE,OAAO,CAAC,IAAI,CAACC,mBAAmB,CAAC;QACvD,IAAI,CAACJ,UAAS,GAAI,IAAI,CAACC,SAAS;QAChC,IAAI,CAACC,OAAM,GAAI,IAAI;QACnB,IAAI,CAACa,qBAAqB,CAAC,CAAC;MAC9B;IACF,CAAC;IAEDE,SAASA,CAAA,EAAG;MACV,IAAI,IAAI,CAACb,mBAAkB,GAAI,IAAI,CAACD,OAAO,CAACW,MAAK,GAAI,CAAC,EAAE;QACtD,IAAI,CAACV,mBAAmB,EAAE;QAC1B,IAAI,CAACH,SAAQ,GAAI,IAAI,CAACE,OAAO,CAAC,IAAI,CAACC,mBAAmB,CAAC;QACvD,IAAI,CAACJ,UAAS,GAAI,IAAI,CAACC,SAAS;QAChC,IAAI,CAACC,OAAM,GAAI,IAAI;QACnB,IAAI,CAACa,qBAAqB,CAAC,CAAC;MAC9B;IACF,C