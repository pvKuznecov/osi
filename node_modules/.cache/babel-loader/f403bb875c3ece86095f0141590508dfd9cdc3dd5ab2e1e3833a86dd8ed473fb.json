his.loading = false;\n      this.error = true;\n    },\n    // Безопасное получение заголовка\n    safeGetTitle() {\n      try {\n        const iframe = this.$refs.iframeRef;\n        if (iframe && iframe.contentDocument) {\n          const title = iframe.contentDocument.title;\n          if (title) {\n            console.log('Page title:', title);\n          }\n        }\n      } catch (e) {\n        console.log('Cannot access iframe content due to CORS');\n      }\n    }\n  },\n  mounted() {\n    console.log('AppWiki mounted with windowId:', this.windowId);\n    // Автоматически сохраняем состояние при монтировании\n    this.saveState();\n  },\n  beforeUnmount() {\n    console.log('AppWiki unmounting');\n    // Решите, нужно ли удалять состояние при закрытии\n    // localStorage.removeItem(`wiki_state_${this.windowId}`);\n  }\n};","map":{"version":3,"names":["appState","name","props","windowId","String","data","savedState","getState","defLink","currentUrl","iframeSrc","loading","error","iframeKey","computed","frameSRC","watch","newUrl","console","log","saveState","methods","state","localStorage","setItem","JSON","stringify","e","reload","includes","Date","now","onIframeLoad","safeGetTitle","onIframeError","iframe","$refs","iframeRef","contentDocument","title","mounted","beforeUnmount"],"sources":["/home/kup/my_projects/vue/osi/src/apps/learn/AppWiki/AppWiki.vue"],"sourcesContent":["\n<template>\n    <div class=\"browser-app\">    \n        <div class=\"iframe-wrapper\">\n            <iframe \n                ref=\"iframeRef\" \n                :src=\"frameSRC\" \n                @load=\"onIframeLoad\" \n                @error=\"onIframeError\" \n                sandbox=\"allow-scripts allow-same-origin allow-forms allow-popups\" \n                referrerpolicy=\"no-referrer\" \n                :key=\"iframeKey\"\n            ></iframe>\n        </div>    \n        <div v-if=\"loading\" class=\"loading-indicator\">\n            <div class=\"spinner\"></div>\n     